<div class="form-row">
  <div class='form-group col-md-12' style='border-bottom: 1px solid;'>
    <h5 class='fs-14'>Dimensions</h5>
  </div>
  <div class="form-group col-md-6 required">
      <%= f.label :length %>
      <%= f.number_field :length, step: 0.01, class: 'form-control', placeholder: 'Enter Length in (mm)', required: true %>
  </div>
  <div class="form-group col-md-6 required">
      <%= f.label :width %>
      <%= f.number_field :width, step: 0.01, class: 'form-control', placeholder: 'Enter Width in (mm)', required: true %>
  </div>
  <div class="form-group col-md-6 required">
      <%= f.label :height %>
      <%= f.number_field :height, step: 0.01, class: 'form-control', placeholder: 'Enter Height in (mm)', required: true %>
  </div>
  <div class="form-group col-md-6 required">
      <%= f.label :weight %>
      <%= f.number_field :weight, step: 0.01, class: 'form-control', placeholder: 'Enter Weight in (g)', required: true %>
  </div>
</div>
