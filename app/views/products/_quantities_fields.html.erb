<div class="form-row">
  <div class="form-group col-md-6">
      <%= f.label :total_stock %>
      <%= f.number_field :total_stock, step: :any, class: 'form-control', placeholder: 'Enter Total Quantity' %>
  </div>
  <div class="form-group col-md-6">
      <%= f.label :fake_stock %>
      <%= f.number_field :fake_stock, step: :any, class: 'form-control', placeholder: 'Enter Fake Stock Quantity' %>
  </div>
  <div class="form-group col-md-6">
      <%= f.label :pending_orders %>
      <%= f.number_field :pending_orders, step: :any, class: 'form-control', placeholder: 'Enter Pending Orders Quantity' %>
  </div>
  <div class="form-group col-md-6">
      <%= f.label :allocated_orders %>
      <%= f.number_field :allocated_orders, step: :any, class: 'form-control', placeholder: 'Enter Allocated Quantity' %>
  </div>
</div>
<div class='form-row text-right clearfix'>
  <button type='button' class='btn btn-sm btn-info ml-auto next-tab-button col-2 offset-10 float-right'>Next</button>
</div>
