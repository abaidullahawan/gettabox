<% if @field_names.present? %>
    <% @field_names&.each do |field_name| %>
        <div class="form-group ">
            <%= f.label :"#{field_name}" %>
            <% extra_value = @product.extra_field_value&.field_value.present? ? @product.extra_field_value&.field_value[field_name.to_s] : ""%>
            <%= f.text_field :"#{field_name}", value: extra_value, class: 'form-control', placeholder: "Enter #{field_name}" %>
        </div>
    <% end %>
<% end %>
