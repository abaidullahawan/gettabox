<div class="form-group">
  <%= f.label :season_id %>
  <%= f.select :season_id, @seasons&.map{|u| [u['name'], u['id']]}, {include_blank: 'Select the season'}, class: 'form-control chosen-select'%>
</div>

<div class="form-group">
  <%= f.label :pack_quantity %>
  <%= f.text_field :pack_quantity, class: 'form-control', placeholder: 'Enter Pack Quantity' %>
</div>

<div class="form-group">
  <%= f.label :cost_price %>
  <%= f.text_field :cost_price, class: 'form-control', placeholder: 'Enter Cost Price' %>
</div>
