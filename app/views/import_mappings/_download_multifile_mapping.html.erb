<div class="col-lg-12">
  <div class="card text-center mb-30">
    <table class="table m-0 table-hover">
      <thead>
        <tr>
          <th class='border-right'>Created At</th>
          <th class='border-right'>Download</th>
        </tr>
      </thead>
        <tbody>
          <% @multifile_mapping.each do |multifile_maping| %>
            <% if multifile_maping.include? 'multi-mapping'%>
              <tr>
                <td><%= multifile_maping.split('/').last.split('--').last %></td>
                <td class='border-right'>
                  <%= link_to "Download", download_path(download: true, url: multifile_maping), download: '', class: "btn btn-primary text-white" %>
                  <%= link_to "Delete", download_path(download: false, url: multifile_maping), class: "btn btn-danger text-white", data: {confirm: "Are you sure?"} %>
                </td>
              </tr>
            <% end %>
          <% end %>
        </tbody>
    </table>
  </div>
</div>