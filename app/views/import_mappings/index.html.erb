                        <div class="card text-center mb-30">
                          <div class="header card-header">Mapped</div>
                          <table class="table m-0 table-hover">
                            <thead>
                              <tr>
                                <th>Table Field</th>
                                <th class='border-right'>Header Field</th>
                                <th>Table Field</th>
                                <th class='border-right'>Header Field</th>
                                <th>Table Field</th>
                                <th class='border-right'>Header Field</th>
                                <th>Table Field</th>
                                <th class='border-right'>Header Field</th>
