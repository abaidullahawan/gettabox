<p id="notice"><%= notice %></p>

<h1>Import Mappings</h1>

<table>
  <thead>
    <tr>
      <th>Table name</th>
      <th>description</th>
      <th>Mapping data</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @import_mappings.each do |import_mapping| %>
      <tr>
        <td><%= import_mapping.table_name %></td>
        <td><%= import_mapping.description %></td>
        <td>
          <table>
            <thead>
              <tr>
                <th>Table Field</th>
                <th>Header Field</th>
              </tr>
            </thead>
            <tbody>
              <% import_mapping.mapping_data.each do |mapping| %>
                <tr>
                  <td><%= mapping[0] %></td>
                  <td><%= mapping[1] %></td>
                </tr>
              <% end %>
            </tbody>
          </table>
        </td>
        <td><%= link_to 'Destroy', import_mapping, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn-danger" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

