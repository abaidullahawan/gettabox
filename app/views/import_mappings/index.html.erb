                                    <td class='border-right'><%= maps[1] %></td>
                                  <% end %>
                                </tr>
                                <% end %>
                              </tbody>
                          </table>
                        </div>
                      </div>

                      <div class="col-lg-12">
                        <div class="card text-center mb-30">
                          <div class="header card-header">Unmapped</div>
                          <table class="table m-0 table-hover">
                            <thead>
                              <tr>
                                <th class='border-right'>Header Field</th>
