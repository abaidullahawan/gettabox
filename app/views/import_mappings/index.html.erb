          <!-- Multi File Accordion -->
          <div class="faq-accordion mb-30">
            <ul class="accordion">
              <li class="accordion-item">
                <a class="accordion-title" href="javascript:void(0)">
                  <i class="lni-plus"></i>
                  Muliple File Matching and Mapping
                </a>
                <div class="accordion-content">
                  <h4 class='mb-3'>New Mapping</h4>
                  <div class= "mb-4 border">
                    <%= form_with url: file_mapping_path, method: :post do |form|%>
                      <%= file_field_tag "file_1", :accept => 'csv', required: true%>
                      <%= form.file_field "file_2", :accept => 'csv', required: true%>
                      <%= form.submit 'Create Mapping', class:"btn btn-primary"%>
                    <% end %>
                  </div>
                  <h4 class='mb-3'>Mappings</h4>
                  <% @multi_mappings.each do |import_mapping| %>
                    <div class='col-12 mb-2 float-right'>
                      <h1 class="fs-18 mb-2 mt-2"><strong>Description: </strong><%= import_mapping.sub_type %></h1>
                      <div class= 'd-flex'>
                        <%= link_to 'Destroy', import_mapping, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger" %>
                        <%= link_to 'Edit', edit_import_mapping_path(import_mapping), class: "btn btn-info ml-3 mr-3" %>
                        <%= form_tag multi_file_path, local:true, multipart: true, method: :post do%>
                          <%= file_field_tag "file_1", :accept => 'csv', required: true%>
                          <%= file_field_tag "file_2", :accept => 'csv', required: true%>
                          <%= hidden_field_tag 'mapping_id', import_mapping.id%>
                          <%= submit_tag 'Create Mapping', class:"btn btn-primary"%>
                        <% end %>
                      </div>
                      <% @blank, @full = import_mapping&.mapping_data&.partition { |_, v| v.blank? }&.map { |alist| Hash[alist] }%>
