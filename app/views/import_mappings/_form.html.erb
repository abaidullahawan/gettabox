<%= form_with url: '/import_mappings', remote: true do |f| %>
  <div class="form-group">
    <%= f.label :table_name %>
    <%= f.select :table_name,  @table_names, {}, class: 'form-control'%>
  </div>
  <% db_columns.each do |col_name| %>
  <div class="d-flex">
    <div class="form-group">
      <%= label_tag :"#{col_name}" %>
      <%= text_field_tag(col_name,  value = col_name,:readonly => true, class: 'form-control')%>
    </div>
    <div class="form-group">
      <%= label_tag :header_fields %>
     <%= select_tag :header_fields,  options_for_select(header), class: 'form-control'%>
    </div>
  </div>
  <% end %>

  <div class="form-group">
    <%= f.submit %>
  </div>
<% end %>
