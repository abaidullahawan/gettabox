<!-- Main Content Header -->
<div class="main-content-header">
  <h1>Rules</h1>
  <ol class="breadcrumb">
    <li class="breadcrumb-item">
      <a href="<%= home_path %>">Dashboard</a>
    </li>
    <li class="breadcrumb-item">
      <a href="<%= settings_path %>">Settings</a>
    </li>
    <li class="breadcrumb-item active">
      <a href="<%= rules_path %>">Rules</a>
    </li>
    <li class="breadcrumb-item active">
      <span class="active">View</span>
    </li>
  </ol>
</div>
<!-- End Main Content Header -->
<div class="row">
  <div class="col-xl-12">
    <div class="card mb-30">
      <div class="card-body">
        <div class="card-header">
          <h5 class="card-title">
            View
          </h5>
        </div>
        <%# <fieldset disabled> %>
        <%= form_for @rule, url: rules_path(@rule) do |f| %>
          <fieldset disabled>
            <%= render 'form', f: f %>
          </fieldset>
          <%#= f.submit 'Update', class: 'btn btn-success mt-4 float-right'%>
        <% end %>
      </div>
    </div>
  </div>
</div>
